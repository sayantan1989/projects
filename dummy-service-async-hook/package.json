{
    "name": "where-used-service",
    "version": "0.0.1",
    "description": "Where Used Service",
    "scripts": {
        "build": "npm-run-all --sequential clean build:proto:gen tsc cp",
        "build:proto:gen": "./scripts/generate_proto_proxies.sh",
        "clean": "rimraf ./report ./dist ./.nyc_output",
        "start": "cross-env NODE_ENV=development ts-node --files ./src/index.ts",
        "start:dev": "cross-env NODE_ENV=development ts-node --files ./src/index.ts",
        "lint": "eslint \"@(src|test)/**/*.ts\"",
        "test": "cross-env NODE_ENV=test TS_NODE_FILES=true mocha",
        "tsc": "tsc",
        "cp": "copyfiles protos/**/*.* protos/**/*.proto src/resources/**/* dist/"
    },
    "private": true,
    "engines": {
        "node": ">=14.20.0 <15.0.0"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.7.0",
        "ajv": "^8.11.0",
        "axios": "^1.3.2",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "crypto": "^1.0.1",
        "elastic-apm-node": "^3.42.0",
        "express": "^4.17.2",
        "google-protobuf": "^3.19.4",
        "grpc": "^1.24.11",
        "helmet": "^5.1.0",
        "jsonwebtoken": "^8.5.1",
        "lightship": "^7.0.2",
        "lodash": "^4.17.21",
        "mali": "^0.47.1",
        "mongoose": "^6.2.10",
        "node-fetch": "^2.6.7",
        "uuid": "^8.3.2",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.1",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/lodash": "^4.14.191",
        "@types/mocha": "^9.1.1",
        "@types/node": "^14.14.31",
        "@types/node-fetch": "^2.6.1",
        "@types/sinon": "^10.0.11",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "chai": "^4.3.6",
        "chai-http": "^4.0.0",
        "copyfiles": "^2.4.1",
        "delay": "^5.0.0",
        "eslint": "^8.9.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "eslint-plugin-sonarjs": "^0.6.0",
        "grpc-mock": "^0.7.0",
        "grpc-tools": "^1.11.2",
        "istanbul": "^0.4.5",
        "karma-sonarqube-reporter": "^1.4.0",
        "mocha": "^9.2.2",
        "mocha-sonarqube-reporter": "^1.0.2",
        "nodemon": "^2.0.6",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "rewiremock": "^3.14.3",
        "rimraf": "^3.0.2",
        "sinon": "^9.2.4",
        "sonar-scanner": "^3.1.0",
        "source-map-support": "^0.5.19",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^1.1.6",
        "ts-protoc-gen": "^0.15.0",
        "typescript": "^4.8.4"
    }
}
